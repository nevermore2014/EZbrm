<%= form_for [@user, @location] do |f| %>
<div class="container" style="margin-left:163px">
  <p>
    <%= f.label :name %></br>
    <%= f.text_field :name %></br>
    <%= f.label :category %></br>
    <%= f.text_field :category %></br>
    <%= f.label :address %></br>
    <%= f.text_field :address %>
  </p>
  <p><%= f.submit %></p>
</div>
<% end %>
